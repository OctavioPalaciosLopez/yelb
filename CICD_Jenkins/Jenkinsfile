pipeline{
    agent any
    options{
        skipStagesAfterUnstable()
    }
    stages{
        stage('Checking Repository'){
            steps{
                checkout scm
            }
            
        }
        stage('Build Docker Image'){
            steps{
                script{
                    app = docker.build("yelb/nginx", "/yelb-ui")
                }
            }   
        }
    }
}